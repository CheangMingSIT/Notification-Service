{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/notification-api/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/notification-api/tsconfig.app.json"
  },
  "projects": {
    "notification-api": {
      "type": "application",
      "root": "apps/notification-api",
      "entryFile": "main",
      "sourceRoot": "apps/notification-api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/notification-api/tsconfig.app.json"
      }
    },
    "worker-service": {
      "type": "application",
      "root": "apps/worker-service",
      "entryFile": "main",
      "sourceRoot": "apps/worker-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/worker-service/tsconfig.app.json",
        "assets": [
          "template/*.hbs"
        ],
        "watchAssets": true
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "notification-log-api": {
      "type": "application",
      "root": "apps/notification-log-api",
      "entryFile": "main",
      "sourceRoot": "apps/notification-log-api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/notification-log-api/tsconfig.app.json"
      }
    },
    "dlx-notification-api": {
      "type": "application",
      "root": "apps/dlx-notification-api",
      "entryFile": "main",
      "sourceRoot": "apps/dlx-notification-api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/dlx-notification-api/tsconfig.app.json"
      }
    },
    "auth": {
      "type": "library",
      "root": "libs/auth",
      "entryFile": "index",
      "sourceRoot": "libs/auth/src",
      "compilerOptions": {
        "tsConfigPath": "libs/auth/tsconfig.lib.json"
      }
    }
  },
  "monorepo": true,
  "root": "apps/notification-api"
}