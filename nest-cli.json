{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/notification-api/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/notification-api/tsconfig.app.json"
  },
  "projects": {
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "notification-api": {
      "type": "application",
      "root": "apps/notification-api",
      "entryFile": "main",
      "sourceRoot": "apps/notification-api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/notification-api/tsconfig.app.json"
      }
    },
    "message-queue": {
      "type": "application",
      "root": "apps/message-queue",
      "entryFile": "main",
      "sourceRoot": "apps/message-queue/src",
      "compilerOptions": {
        "tsConfigPath": "apps/message-queue/tsconfig.app.json"
      }
    },
    "worker-service": {
      "type": "application",
      "root": "apps/worker-service",
      "entryFile": "main",
      "sourceRoot": "apps/worker-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/worker-service/tsconfig.app.json"
      }
    }
  },
  "monorepo": true,
  "root": "apps/notification-api"
}